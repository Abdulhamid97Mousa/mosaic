# Environment configuration for the Gym GUI project.
# Copy this file to `.env` and adjust values for your local setup.

QT_API=pyqt6
GYM_DEFAULT_ENV=FrozenLake-v1
GYM_VIDEO_DIR=./var/video
ENABLE_AGENT_AUTOSTART=false
LOG_LEVEL=INFO
USE_GPU=false
DEFAULT_CONTROL_MODE=human_only
# Initial seed applied when launching an environment (must be >=1).
GYM_DEFAULT_SEED=1
# Set to true to allow reusing the same seed between runs.
GYM_ALLOW_SEED_REUSE=false
# Comma-separated list of agent identifiers to auto-load.
AGENT_IDS=

# CarRacing defaults (set to 0 to keep Gym's built-in limits)
CAR_RACING_MAX_EPISODE_STEPS=0
CAR_RACING_MAX_EPISODE_SECONDS=300
CAR_RACING_LAP_COMPLETE_PERCENT=0.95
CAR_RACING_CONTINUOUS=false
CAR_RACING_DOMAIN_RANDOMIZE=false

# Bipedal Walker defaults (0 keeps Gym defaults)
BIPEDAL_MAX_EPISODE_STEPS=10000
BIPEDAL_MAX_EPISODE_SECONDS=300
BIPEDAL_HARDCORE=false


# Platform-specific settings
PLATFORM=ubuntu

# Disable Qt plugin debugging
QT_DEBUG_PLUGINS=0 

# gRPC logging verbosity
GRPC_VERBOSITY=debug

# Telemetry sampling interval (default: every 2nd step)
TELEMETRY_SAMPLING_INTERVAL=2